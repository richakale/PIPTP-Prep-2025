#include <stdio.h> 

  int funn(int a, int b) {
     int c; 

    for (c = 2; c <= 4; c++) {
         if ((a % 2) < (b % 3)) {
             a = 4 % 3; // a becomes 1
        } else {
            if ((5 % 3) > b) {
               a = b;
            }
           b = 1;
        }
    }
   return a + b;
}



output : 2
